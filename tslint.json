{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-submodule-imports": false,
    "no-implicit-dependencies": [
      true,
      [
        "dev",
        "$src",
        "$components"
      ]
    ],
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "react-hooks/exhaustive-deps": false
  }
}